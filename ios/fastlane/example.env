# Your Apple email address (e.g. apple@example.com)
APPLE_ID=

# The bundle identifier of your app (e.g. com.example.myApp)
APP_IDENTIFIER=

# App-specific password used for uploads (generate at https://appleid.apple.com/account/manage)
FASTLANE_APPLE_APPLICATION_SPECIFIC_PASSWORD=

# Main scheme to build
GYM_SCHEME=

# App Store Connect Team ID (e.g. 123456789)
ITC_TEAM_ID=

# Path to main project file (e.g. MyApp.xcodeproj, relative to /ios)
MAIN_PROJECT=

# Path to main workspace (e.g. MyApp.xcworkspace, relative to /ios)
MAIN_WORKSPACE=

# Apple ID assigned to the app (e.g. 1234567890, find under App Store Connect -> <your app> -> App Information, https://appstoreconnect.apple.com/apps/)
PILOT_APPLE_ID=

# Developer Portal Team ID (e.g. ABCDE1FGH2)
TEAM_ID=

# Omit to use short upload (will update changelog but skip waiting for distribution which can take 10+ minutes)
PERFORM_LONG_UPLOAD=true

# Can be used to force the version number string to use
# FORCED_VERSION_NUMBER_STRING=1.0


# The following env variables are only needed for CI systems

# Base64-encoded contents of production certificate .p12 file (https://docs.github.com/en/actions/guides/installing-an-apple-certificate-on-macos-runners-for-xcode-development)
# BUILD_CERTIFICATE_BASE64=

# User-generated password to use for temp keychain
# CERT_KEYCHAIN_PASSWORD=

# Password for APPLE_ID account
# FASTLANE_PASSWORD=

# Fastlane session token (generate with `fastlane spaceauth -u <apple_id>`, https://docs.fastlane.tools/best-practices/continuous-integration/)
# FASTLANE_SESSION=

# Password to decode BUILD_CERTIFICATE_BASE64
# P12_PASSWORD=

# User-generated random string; changing it forces a rebuild of the pods derived data cache on the next build
# PODS_DD_PREFIX=

# The full content of this variable will be placed as .env in the root of the project before running Fastlane
# ADDITIONAL_ENV_VARS=
